<Window x:Class="Database.View.Query.TableChartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Database.View.Query"
        mc:Ignorable="d"
        Title="Запрос с таблицей и графиком" Height="768" Width="1366">
    <Grid Margin="10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Frame Content="{Binding DataGridPage}" NavigationUIVisibility="Hidden"/>
        <Frame Grid.Column="1" Content="{Binding ChartPage}" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
